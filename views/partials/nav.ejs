  <% if (!user) { %>
    <link rel="stylesheet" href="/stylesheets/siwg.css">
  <% } %>
</head>
<body>
  <nav>
    <a 
			href="/trails/new"
			<%- title === 'Add Trail' ? 'class="active"' : '' %>
		>
			Add New Trail
		</a>  
    <a 
      href="/hikes/show" <%- title === 'My Hikes' ? 'class="active"' : '' %>
    >
      My Hikes
    </a>
    <a 
      href="/hikes" <%- title === 'Explore Hood Mountain' ? 'class="active"' : '' %>
    >
      Explore Hood Mountain
    </a>
    <% if (user) { %>
      <a href="/auth/logout">
        <button type="submit">Logout</button>
      </a>
    <% } else { %>
      <a href="/auth/google" class="siwg-link">
        <button type="submit" class="siwg">
          <img id="siwg-image" alt="Sign in with Google" />
        </button>
      </a>
    <% } %>
  </nav>
